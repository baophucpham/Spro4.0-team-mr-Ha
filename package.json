{
	"name": "AwesomeProject",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"lint": "eslint .",
		"start": "react-native start",
		"test": "jest",
		"check-info-plist": "cd ios && plutil ./happyhiv/Info.plist",
		"link-assets": "npx react-native-asset",
		"pod-install": "cd ios && react-native setup-ios-permissions && bundle install && bundle exec pod install && cd ..",
		"all": "rm -rf node_modules && yarn && yarn pod-install && yarn link-assets",
		"clean-android": "cd android && set ENVFILE=.env && ./gradlew clean",
		"build-android": "cd android && npx gradlew assembleRelease",
		"update-android": "appcenter codepush release-react -a -d Production",
		"update-ios": "appcenter codepush release-react -a -d Production"
	},
	"dependencies": {
		"@react-native-async-storage/async-storage": "^1.17.11",
		"@react-native-firebase/analytics": "^17.4.0",
		"@react-native-firebase/app": "^17.4.0",
		"@react-native-firebase/auth": "^17.4.0",
		"@react-native-google-signin/google-signin": "^9.0.2",
		"@react-native-masked-view/masked-view": "^0.2.8",
		"@react-navigation/bottom-tabs": "^6.5.7",
		"@react-navigation/native": "^6.1.6",
		"@react-navigation/native-stack": "^6.9.12",
		"@react-navigation/stack": "^6.3.16",
		"@reduxjs/toolkit": "^1.9.3",
		"axios": "^1.3.4",
		"babel-plugin-module-resolver": "^5.0.0",
		"date-fns": "^2.29.3",
		"lodash": "^4.17.21",
		"react": "18.2.0",
		"react-native": "0.71.4",
		"react-native-date-picker": "^4.2.9",
		"react-native-fast-image": "^8.6.3",
		"react-native-fbsdk-next": "^11.2.0",
		"react-native-gesture-handler": "^2.9.0",
		"react-native-image-picker": "^5.1.0",
		"react-native-localization": "^2.3.1",
		"react-native-permissions": "^3.7.3",
		"react-native-safe-area-context": "^4.5.0",
		"react-native-screens": "^3.20.0",
		"react-redux": "^8.0.5",
		"redux-saga": "^1.2.2"
	},
	"devDependencies": {
		"@babel/core": "^7.20.0",
		"@babel/preset-env": "^7.20.0",
		"@babel/runtime": "^7.20.0",
		"@react-native-community/eslint-config": "^3.2.0",
		"@tsconfig/react-native": "^2.0.2",
		"@types/jest": "^29.2.1",
		"@types/lodash": "^4.14.191",
		"@types/react": "^18.0.24",
		"@types/react-test-renderer": "^18.0.0",
		"babel-jest": "^29.2.1",
		"eslint": "^8.19.0",
		"jest": "^29.2.1",
		"metro-react-native-babel-preset": "0.73.8",
		"pod-install": "0.1.38",
		"prettier": "^2.4.1",
		"react-test-renderer": "18.2.0",
		"typescript": "4.8.4"
	},
	"jest": {
		"preset": "react-native"
	},
	"reactNativePermissionsIOS": [
